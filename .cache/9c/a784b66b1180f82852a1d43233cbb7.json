inal":{"line":42,"column":2},"generated":{"line":56,"column":3}},{"source":"../utils/bem.ts","name":null,"original":{"line":51,"column":1},"generated":{"line":57,"column":0}}],"sources":{"../utils/bem.ts":"/**\n * bem helper\n * b() // 'button'\n * b('text') // 'button__text'\n * b({ disabled }) // 'button button--disabled'\n * b('text', { disabled }) // 'button__text button__text--disabled'\n * b(['disabled', 'primary']) // 'button button--disabled button--primary'\n */\n\nexport type Mod = string | { [key: string]: any };\nexport type Mods = Mod | Mod[];\n\nconst ELEMENT = '__';\nconst MODS = '--';\n\nfunction join(name: string, el?: Mods, symbol?: string): string {\n  return el ? name + symbol + el : name;\n}\n\nfunction prefix(name: string, mods: Mods): Mods {\n  if (typeof mods === 'string') {\n    return join(name, mods, MODS);\n  }\n\n  if (Array.isArray(mods)) {\n    return mods.map((item) => <Mod>prefix(name, item));\n  }\n\n  const ret: Mods = {};\n  if (mods) {\n    Object.keys(mods).forEach((key) => {\n      ret[name + MODS + key] = mods[key];\n    });\n  }\n\n  return ret;\n}\n\nexport function createBEM(name: string) {\n  name = 't-' + name;\n  // t-button\n  return function (el?: Mods, mods?: Mods): Mods {\n    if (el && typeof el !== 'string') {\n      mods = el;\n      el = '';\n    }\n    el = join(name, el, ELEMENT);\n\n    return mods ? [el, prefix(el, mods)] : el;\n  };\n}\n\nexport type Bem = ReturnType<typeof createBEM>;\n"},"lineCount":null}},"error":null,"hash":"d76b41898108912d544c1928bd23fee5","cacheData":{"env":{}}}