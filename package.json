{
  "name": "vue-next-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "rm -rf .cache && parcel examples/index.html",
    "build": "rm -rf dist npx rollup -c",
    "lint": "prettier --write  \"packages/**/*.ts?(x)\" \"example/**/*.ts?(x)\"",
    "commit": "git-cz",
    "release": "release-it",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "peerDependencies": {
    "vue": "^3.0.0-beta.14"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.2.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "parcel-bundler": "^1.12.4",
    "release-it": "^13.6.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.7.4",
    "vue": "^3.0.0-beta.14",
    "vue-loader": "^16.0.0-beta.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "alias": {
    "packages": "./packages"
  },
  "dependencies": {}
}
